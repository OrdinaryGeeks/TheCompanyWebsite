      <aspNetCore processPath="dotnet" arguments=".\OrdinaryGeeks.dll" stdoutLogEnabled="true" stdoutLogFile=".\logs\stdout" hostingModel="inprocess" />
    </system.webServer>
  </location>
</configuration>
